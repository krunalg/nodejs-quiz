<% include ./header.ejs %>
	<div class="container">
		<div class="card text-center">
			<div class="card-header">
				Demo Quiz
			</div>
			<div class="card-body">
				<h5 class="card-title">
					Your Scores
				</h5>
				<div class="row">
					<div class="col-8 quizForm">
						<table class="table table-striped">
							<thead class="thead-dark">
								<tr>
									<th>Total Questions</th>
									<th>Your Score</th>
								</tr>
							</thead>
							<tbody>
								<tr class="success">
									<td>
										<%= total %>
									</td>
									<td>
										<%= score %>
									</td>
								</tr>
							</tbody>
						</table>
						<a href="/" class="btn btn-success pull-left">Play Again</a>
					</div>
				</div>
			</div>
			<div class="card-footer text-muted">
				&nbsp;
			</div>
		</div>
	</div>
	|
	<% include ./footer.ejs %>